<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
 <servlet>
  <servlet-name>myServlet</servlet-name>
  <servlet-class>lesson06.Servlet</servlet-class>
</servlet>
 <servlet>
  <servlet-name>docServlet</servlet-name>
  <servlet-class>docControllers.DocController</servlet-class>
</servlet>
<servlet>
  <servlet-name>user</servlet-name>
  <servlet-class>userController.UserController</servlet-class>
</servlet>
<servlet>
  <servlet-name>regist</servlet-name>
  <servlet-class>regController.RegController</servlet-class>
</servlet>
<servlet-mapping>
  <servlet-name>myServlet</servlet-name>
  <url-pattern>/Servlet</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>docServlet</servlet-name>
  <url-pattern>/docservlet</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>user</servlet-name>
  <url-pattern>/authorization</url-pattern>
</servlet-mapping>
<servlet-mapping>
  <servlet-name>regist</servlet-name>
  <url-pattern>/registration</url-pattern>
</servlet-mapping>
</web-app>
